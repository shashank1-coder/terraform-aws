{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Sample EC2 instance using CloudFormation",
    "Resources": {
        "EC2Instance": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "ImageId": "ami-0e35ddab05955cf57",  
                "InstanceType": "t2.micro",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "Jenkins-CF-Instance"
                    }
                ]
            }
        }
    }
}
